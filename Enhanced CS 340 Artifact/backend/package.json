{
  "name": "grazioso-salvare-backend",
  "version": "1.0.0",
  "description": "Backend API for Grazioso Salvare Animal Rescue Dashboard",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\"",
    "benchmark:breed": "node benchmarks/breedHashTableBenchmark.js",
    "benchmark:bst": "node benchmarks/bstBenchmark.js"
